pipeline {
    agent any

    stages {
        stage('STARTING') {
            steps {
                echo 'i am starting a pipeline CICD'
            }
        }
        
        stage('BUILD') {
            steps {
                echo 'Developement build'
            }
        }
        
        stage('TEST') {
            steps {
                echo 'testing my code'
            }
        }
        
        stage('DEPLOY') {
            steps {
                echo 'Deployment to production'
            }
        }
    }
}
